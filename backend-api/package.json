{
    "name": "insurance-backend",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "nodemon ./bin/www",
        "dev": "set NODE_ENV=development && nodemon ./bin/www",
        "dev:r": "node swagger.js && set NODE_ENV=development && nodemon ./bin/www",
        "create-swagger": "node swagger.js",
