    "dependencies": {
        "@supabase/supabase-js": "^2.45.1",
        "bcrypt": "^5.1.1",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "express-validator": "^7.1.0",
        "http-errors": "~1.6.3",
        "pug": "2.0.0-beta11",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "morgan": "~1.9.1",
        "multer": "^1.4.5-lts.1",
        "nodemon": "^3.1.4",
        "swagger-autogen": "^2.23.7",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.14.1"
    },
    "devDependencies": {
        "husky": "^9.1.6",
